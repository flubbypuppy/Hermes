{
  "name": "vivid-studio",
  "description": "Build visually and code faster with Vivid — an in browser visual editor to modify your code",
  "version": "0.12.0",
  "files": [
    "dist/*",
    "index.d.ts",
    "style.css",
    "style.css.d.ts"
  ],
  "main": "./dist/vivid-studio.mjs",
  "exports": {
    ".": {
      "import": "./dist/vivid-studio.mjs"
    },
    "./style.css": {
      "import": "./style.css"
    }
  },
  "scripts": {
    "build": "node ./scripts/print-logo.js && yarn build:js && yarn build:css && yarn build:prefix",
    "build:js": "echo '┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓\n       🔥 Building JS 🔥' && yarn remove-dist && node ./scripts/build.js && yarn rename && echo '┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛\n'",
    "build:css": "echo '┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓\n       ✨ Building CSS ✨' && tailwindcss -i ./src/style/globals.css -o ./dist/style.css -c './tailwind.config.build.js' --minify && echo '┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛\n'",
    "build:prefix": "echo '┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓\n      🦄 Prefixing CSS 🦄' && node ./scripts/prefix-css.js && sass --no-source-map ./dist/style.scss ./style.css && yarn remove-scss && echo '┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛\n'",
    "rename": "node -e \"require('fs').rename('./dist/runtime.mjs', './dist/vivid-studio.mjs', function(err) {if (err) console.log(err);});\"",
    "remove-scss": "node -e \"require('fs').unlink('./dist/style.scss', function(err) {if (err) console.log(err);});\"",
    "remove-dist": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true });\"",
    "dev": "vite",
    "tailwind-config": "tailwind-config-viewer -o"
  },
